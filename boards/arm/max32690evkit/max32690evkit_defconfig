CONFIG_SOC_SERIES_MAX32=y
CONFIG_SOC_MAX32690=y

# Enable GPIO
CONFIG_GPIO=y

CONFIG_SYS_CLOCK_HW_CYCLES_PER_SEC=120000000

CONFIG_PINCTRL=y

# Console
CONFIG_CONSOLE=n
CONFIG_UART_CONSOLE=n

# enable uart driver
CONFIG_SERIAL=n
CONFIG_UART_MAX32=n

# Enable Clocks
CONFIG_CLOCK_CONTROL=y

# Enable I2C
CONFIG_I2C=y

# Enable SPI
CONFIG_SPI=y
