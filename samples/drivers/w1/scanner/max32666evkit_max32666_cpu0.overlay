&owm_io_p0_4 {
	power-source=<MAX32_VSEL_VDDIOH>;
};

&owm_pe_p0_5 {
	power-source=<MAX32_VSEL_VDDIOH>;
};

&w1 {
	status = "okay";
	pinctrl-0 = <&owm_io_p0_4 &owm_pe_p0_5>;
	pinctrl-names = "default";
	internal-pullup = <1>;
	external-pullup = <0>;
	long-line-mode = <0>;
};
